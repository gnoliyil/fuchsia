{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            include: "../loopback_tcp_accept_filter.json5",
        },
        {
            type: "expect_failure",
            matchers: [
                "All/SocketInetLoopbackTest.AcceptedInheritsTCPUserTimeout/*",
                "All/SocketInetLoopbackTest.TCPAcceptAfterReset/*",
                "All/SocketInetLoopbackTest.TCPDeferAccept/*",
                "All/SocketInetLoopbackTest.TCPDeferAcceptTimeout/*",
            ],
        },
        {
            type: "expect_pass",
            matchers: [
                "All/SocketInetLoopbackTest.AcceptedInheritsTCPUserTimeout/ListenV4Any_ConnectV4Any",
                "All/SocketInetLoopbackTest.AcceptedInheritsTCPUserTimeout/ListenV4Any_ConnectV4Loopback",
                "All/SocketInetLoopbackTest.AcceptedInheritsTCPUserTimeout/ListenV4Loopback_ConnectV4Any",
                "All/SocketInetLoopbackTest.AcceptedInheritsTCPUserTimeout/ListenV4Loopback_ConnectV4Loopback",
                "All/SocketInetLoopbackTest.AcceptedInheritsTCPUserTimeout/ListenV6Any_ConnectV6Any",
                "All/SocketInetLoopbackTest.AcceptedInheritsTCPUserTimeout/ListenV6Any_ConnectV6Loopback",
                "All/SocketInetLoopbackTest.AcceptedInheritsTCPUserTimeout/ListenV6Loopback_ConnectV6Any",
                "All/SocketInetLoopbackTest.AcceptedInheritsTCPUserTimeout/ListenV6Loopback_ConnectV6Loopback",
            ],
        },
    ],
}
