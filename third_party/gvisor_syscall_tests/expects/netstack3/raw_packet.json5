{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                // TODO(https://fxbug.dev/106735): Support packet sockets.
                "AllPacketSocketTests/PacketSocketTest.RebindProtocol/*",
            ],
        },
        {
            type: "skip",
            matchers: [
                // TODO(https://fxbug.dev/110824): Stop skipping these manually once NS3 is
                // returning EPERM for raw and packet socket creation.
                "AllInetTests/RawPacketTest.*",
                "AllInetTests/CookedPacketTest.*",
                "AllRawPacketMsgSizeTest/*",
                "BasicCookedPacketTest.WrongType",
                "RawHDRINCL.*",
                "RawSocketICMPTest.*",
                "RawSocketICMPv6Test.*",
                "AllInetTests/RawSocketTest.*",
                "AllRawSocketTests/*",
                "RawSocketTest.*",
            ],
        },
    ],
}
