    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                // https://fxbug.dev/42692
                "BlockingTCPSockets/BlockingStreamSocketPairTest.RecvLessThanBufferWaitAll/*",
            ],
        },
        {
            type: "skip",
            matchers: [
                // https://fxbug.dev/46211
