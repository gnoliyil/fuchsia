{
    children: [
        {
            name: "netstack",
            url: "#meta/netstack3-debug.cm",
        },
    ],
    use: [
        {
            protocol: [ "fuchsia.posix.socket.Provider" ],
            from: "#netstack",
        },
    ],
    offer: [
        {
            protocol: [ "fuchsia.logger.LogSink" ],
            from: "parent",
            to: "#netstack",
        },
    ],
}
