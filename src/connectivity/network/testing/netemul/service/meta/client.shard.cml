{
    children: [
        {
            name: "sandbox",
            url: "#meta/netemul-sandbox.cm",
        },
    ],
    use: [
        {
            protocol: "fuchsia.netemul.Sandbox",
            from: "#sandbox",
        },
    ],
    offer: [
        {
            protocol: [
                "fuchsia.inspect.InspectSink",
                "fuchsia.logger.LogSink",
                "fuchsia.process.Launcher",
            ],
            from: "parent",
            to: "#sandbox",
        },
        {
            storage: [
                "cache",
                "data",
                "tmp",
            ],
            from: "parent",
            to: "#sandbox",
        },
    ],
}
