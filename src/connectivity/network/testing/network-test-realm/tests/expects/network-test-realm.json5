{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure_with_err_logs",
            matchers: [
                // TODO(https://fxbug.dev/47321): Implement ICMP sockets in Netstack3.
                "ping::ipv?_netstack3",
                "ping::ipv?*_bind_to_non_existent_interface_netstack3",
                "ping::ipv?*_bind_to_existing_interface_netstack3",
                "ping::oversized_payload_length_netstack3",
                "ping::host_unreachable_netstack3",
                "ping::no_timeout*_netstack3",
                "ping::timeout_exceeded_netstack3",
                "ping_with_no_added_interface*netstack3*",
            ],
        },
        {
            type: "expect_pass_with_err_logs",
            matchers: [
                // TODO(https://fxbug.dev/23365): Resolve `IP_ADD_MEMBERSHIP` error code
                // discrepancy.
                "join_multicast_group_with_non_existent_interface*netstack_v3_expects",
            ],
        },
    ],
}
