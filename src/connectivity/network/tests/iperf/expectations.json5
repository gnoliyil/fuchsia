{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                // TODO(https://fxbug.dev/115456): enable when Netstack3
                // supports running iPerf in UDP mode.
                "loopback_ns3_*::udp",
            ],
        },
    ],
}
