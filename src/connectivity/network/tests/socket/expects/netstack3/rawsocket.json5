{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                // TODO(https://fxbug.dev/106736): Support raw sockets.
                "RawSocketICMPv6Test.*",
                "AllRawSocketTests/*",
                "RawSocketTest.UnsupportedOps",
                "RawSocketTest.SendToDifferentProtocolV4",
                "RawSocketTest.SendtoRecvfromV6",
                "RawSocketTest.SendtoRecvfrom",
            ],
        },
    ],
}
