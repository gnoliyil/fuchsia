{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                "inspect_nic_ns3",
            ],
        },
        {
            type: "skip",
            matchers: [
                // Hangs indefinitely.
                "inspect_dhcp_ns3*",

                // Hangs indefinitely.
                "inspect_for_sampler_ns3",

                // Hangs indefinitely.
                "inspect_routing_table_ns3",

                // Hangs indefinitely.
                "inspect_stat_counters_ns3",

                // Hangs indefinitely.
                "inspect_socket_stats_ns3",

                // Hangs indefinitely.
                "inspect_config_ns3*",
            ],
        },
    ],
}
