{
    include: [
        "//sdk/lib/driver/compat/compat.shard.cml",
        "inspect/client.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        runner: "driver",
        compat: "driver/iwlwifi.so",
        bind: "meta/bind/iwlwifi.bindbc",
        colocate: "false",
    },
    capabilities: [
        { service: "fuchsia.wlan.softmac.Service" },
        { service: "fuchsia.wlan.phyimpl.Service" },
    ],
    use: [
        { service: "fuchsia.hardware.pci.Service" },
    ],
    expose: [
        {
            service: "fuchsia.wlan.softmac.Service",
            from: "self",
        },
        {
            service: "fuchsia.wlan.phyimpl.Service",
            from: "self",
        },
    ],
}
