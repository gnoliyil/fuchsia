{
    children: [
        {
            name: "wlanix",
            url: "fuchsia-pkg://fuchsia.com/wlanix#meta/wlanix.cm",
            startup: "eager",
            environment: "#core-env",
        },
    ],
    use: [
        {
            protocol: [ "fuchsia.wlan.wlanix.Wlanix" ],
            from: "#wlanix",
        },
    ],
    offer: [
        {
            protocol: [ "fuchsia.logger.LogSink" ],
            from: "parent",
            to: [ "#wlanix" ],
        },
    ],
}
