library fuchsia.ffx.test;

@discoverable
closed protocol Noop {
    /// Does a noop.
    strict DoNoop() -> ();
};

@discoverable
closed protocol Counter {
    /// Adds one to the internal state.
    strict AddOne() -> ();

    /// Gets the internal state.
    strict GetCount() -> (struct {
        count uint64;
    });
};
