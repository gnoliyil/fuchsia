{
    "ffx": {
        "subtool-search-paths": "$BUILD_DIR/host-tools"
    },
    "log": {
        "dir": [
            "$FFX_LOG_DIR",
            "$FUCHSIA_TEST_OUTDIR/ffx_logs",
            "$CACHE/logs"
        ],
        "enabled": [
            "$FFX_LOG_ENABLED",
            true
        ],
        "level": "info",
        "rotations": 5,
        "rotate_size": 104857600
    },
    "proxy": {
        "timeout_secs": 10.0
    }
}
