library fidl.cpp.wire.bindinggroup.test;

/// A protocol specifically designed to test the binding group interface.
protocol Testable {
    /// Echos a string.
    Echo(struct {
        str string;
    }) -> (struct {
        str string;
    });

    /// Causes the other end to close and execute its close handler.
    Terminate();
};

protocol Foo {};

protocol Bar {};
