library fidl.test.echo;

using zx;

@for_deprecated_c_bindings
closed protocol Echo {
    strict Echo(resource struct {
        process zx.handle:EVENTPAIR;
        thread zx.handle:EVENTPAIR;
    }) -> (struct {
        status zx.status;
    });
    strict Ping();
};
