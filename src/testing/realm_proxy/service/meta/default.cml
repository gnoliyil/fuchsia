{
    include: [
        "//src/testing/realm_proxy/service/meta/realm_factory.shard.cml",
        "//src/testing/realm_proxy/service/meta/realm_proxy.shard.cml",
    ],
    program: {
        runner: "elf",
        binary: "bin/default_test_realm_proxy",
    },
    config: {
        proxied_component_url: {
            type: "string",

            // This matches fuchsia.io.MAX_NAME_LENGTH.
            max_size: 255,
        },
        proxied_protocols: {
            type: "vector",
            max_count: 128,
            element: {
                type: "string",

                // This matches fuchsia.io.MAX_NAME_LENGTH.
                max_size: 255,
            },
        },
    },
}
