{
    include: [ "//src/testing/realm_proxy/service/meta/service.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/default_test_realm_proxy",
    },
    config: {
        proxied_component_url: {
            type: "string",
            max_size: 512,
        },
        proxied_protocols: {
            type: "vector",
            max_count: 128,
            element: {
                type: "string",
                max_size: 512,
            },
        },
    },
}
