{
    include: [ "syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/stub_service",
    },
    capabilities: [
        {
            protocol: [
                "fuchsia.test.EventPairHolder",
                "fuchsia.test.Exposed",
                "fuchsia.test.NotExposed",
            ],
        },
    ],
    expose: [
        {
            protocol: [
                "fuchsia.test.EventPairHolder",
                "fuchsia.test.Exposed",

                // Clarification: this is exposed here, but not by the test realm proxy.
                "fuchsia.test.NotExposed",
            ],
            from: "self",
        },
    ],
}
