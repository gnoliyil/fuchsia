library fuchsia.test;

using zx;

@discoverable
protocol Exposed {
    Call() -> ();
};

@discoverable
protocol NotExposed {
    Call() -> ();
};

@discoverable
protocol EventPairHolder {
    /// Passes the event pair to this EventPairHolder.
    /// When this service goes away, the client's side of the event pair will
    /// be signaled with PEER_CLOSED.
    Hold(resource struct {
        event_pair zx.handle:EVENTPAIR;
    });
};
