{
    children: [
        {
            name: "display-png",
            url: "fuchsia-pkg://fuchsia.com/display-png#meta/display-png.cm",
        },
    ],
    offer: [
        {
            directory: "dev-class",
            from: "parent",
            as: "dev-input-report",
            to: [ "#display-png" ],
            subdir: "input-report",
        },
        {
            directory: "dev-class",
            from: "parent",
            as: "dev-display-coordinator",
            to: [ "#display-png" ],
            subdir: "display-coordinator",
        },
        {
            protocol: "fuchsia.sysmem.Allocator",
            from: "#sysmem_connector",
            to: [ "#display-png" ],
        },
        {
            protocol: "fuchsia.ui.scenic.Scenic",
            from: "#ui",
            to: [ "#display-png" ],
            source_availability: "unknown",
        },
        {
            protocol: "fuchsia.logger.LogSink",
            from: "parent",
            to: [ "#display-png" ],
        },
        {
            storage: "data",
            from: "self",
            to: [ "#display-png" ],
        },
    ],
}
