import("//build/components.gni")

group("android") {
  if (target_cpu == "x64" && !is_asan) {
    deps = [ ":starnix_android" ]
  }
}

fuchsia_component("sh") {
  manifest = "meta/sh.cml"
}

fuchsia_package("starnix_android") {
  deps = [ ":sh" ]
}
