import("//build/components.gni")

group("chromiumos") {
  if (target_cpu == "x64") {
    deps = [ ":starnix_chromiumos" ]
  }
}

fuchsia_component("sh") {
  manifest = "meta/sh.cml"
}

fuchsia_package("starnix_chromiumos") {
  deps = [ ":sh" ]
}
