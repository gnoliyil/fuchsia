{
    actions: [
        {
            type: "expect_pass",
            matchers: [
                "*",
            ],
        },
        {
            type: "expect_failure",
            matchers: [
                "io.ReadFd*",
                "adb_utils.mkdirs*",
                "sysdeps.stat*",
                "io.WriteFdExactly_whole",
                "io.WriteFdExactly_partial",
                "io.WriteFdExactly_string",
                "io.WriteFdFmt",
                "LocalSocketTest.read_from_closing_socket",
                "sysdeps_poll.invalid_fd",
                "ShellServiceTest.RawNoProtocolSubprocess",
                "ShellServiceTest.PtyNoProtocolSubprocess",
                "ShellServiceTest.RawShellProtocolSubprocess",
                "ShellServiceTest.PtyShellProtocolSubprocess",
                "ShellServiceTest.CloseClientStdin",
                "ShellServiceTest.CloseStdinStdoutSubprocess",
                "ShellServiceTest.CloseStderrSubprocess",
            ],
        },
        {
            type: "skip",
            matchers: [
                // These tests currently time out.
                "PropertyMonitor*",
                "sysdeps_fd.*",

                // This test fails but sometimes times out.
                "LocalSocketTest.close_socket_in_CLOSE_WAIT_state",
            ],
        },
    ],
}
