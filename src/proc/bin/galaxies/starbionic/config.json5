{
    apex_hack: [
        "com.android.runtime",
    ],
    features: [
        "magma",
        "binder",
        "framebuffer",
        "selinux_enabled",
    ],
    init: [
        "/system/bin/init",
    ],
    init_user: "root:x:0:0",
    kernel_cmdline: "androidboot.hardware=starnix androidboot.debug.sf.nobootanimation=1",
    mounts: [
        "/:ext4:data/system.img",
    ],
    name: "starbionic",
    startup_file_path: "/dev/binderfs",
}
