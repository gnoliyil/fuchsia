{
    include: [ "syslog/offer.shard.cml" ],
    children: [
        {
            name: "client",
            url: "client#meta/client.cm",
        },
        {
            name: "harness",
            url: "fidl_client_suite_test_harness#meta/harness.cm",
        },
    ],
    offer: [
        {
            protocol: "fidl.clientsuite.Runner",
            from: "#client",
            to: "#harness",
        },
    ],
    expose: [
        {
            protocol: "fuchsia.test.Suite",
            from: "#harness",
        },
    ],
}
