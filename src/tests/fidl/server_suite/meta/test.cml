{
    include: [ "syslog/offer.shard.cml" ],
    children: [
        {
            name: "server",
            url: "server#meta/server.cm",
        },
        {
            name: "harness",
            url: "fidl_server_suite_test_harness#meta/harness.cm",
        },
    ],
    offer: [
        {
            protocol: "fidl.serversuite.Runner",
            from: "#server",
            to: "#harness",
        },
    ],
    expose: [
        {
            protocol: "fuchsia.test.Suite",
            from: "#harness",
        },
    ],
}
