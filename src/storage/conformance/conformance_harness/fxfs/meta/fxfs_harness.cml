{
    include: [
        "inspect/client.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        runner: "elf",
        binary: "bin/io_conformance_harness_fxfs",
        use_next_vdso: "true",
    },
    capabilities: [
        { protocol: "fuchsia.io.test.Io1Harness" },
    ],
    expose: [
        {
            protocol: "fuchsia.io.test.Io1Harness",
            from: "self",
        },
    ],
}
