{
    children: [
        {
            name: "fxfs",
            url: "#meta/fxfs.cm",
        },
    ],
    use: [
        {
            protocol: [
                "fuchsia.fs.Admin",
                "fuchsia.fs.startup.Startup",
            ],
            from: "#fxfs",
        },
        {
            protocol: "fuchsia.component.Realm",
            from: "framework",
        },
    ],
    offer: [
        {
            protocol: [
                "fuchsia.logger.LogSink",
                "fuchsia.process.Launcher",
            ],
            from: "parent",
            to: [ "#fxfs" ],
        },
    ],
}
