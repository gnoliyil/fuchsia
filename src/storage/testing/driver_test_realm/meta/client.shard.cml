{
    children: [
        {
            name: "storage_driver_test_realm",
            url: "#meta/storage_driver_test_realm.cm",
            startup: "eager",
        },
    ],
    use: [
        {
            directory: "dev-topological",
            from: "#storage_driver_test_realm",
            rights: [ "r*" ],
            path: "/dev",
        },
    ],
    offer: [
        {
            protocol: [
                "fuchsia.logger.LogSink",
                "fuchsia.process.Launcher",
            ],
            from: "parent",
            to: "#storage_driver_test_realm",
        },
    ],
}
