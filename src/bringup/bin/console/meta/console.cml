{
    program: {
        runner: "elf",
        binary: "bin/console",
        forward_stdout_to: "none",
        forward_stderr_to: "none",
    },
    capabilities: [
        { protocol: "fuchsia.hardware.pty.Device" },
    ],
    use: [
        {
            protocol: [
                "fuchsia.boot.WriteOnlyLog",
                "fuchsia.kernel.DebugResource",
            ],
        },
    ],
    expose: [
        {
            protocol: "fuchsia.hardware.pty.Device",
            from: "self",
        },
    ],
}
