    // verifying config in product assembly checks that a supported runner is specified
    program: {
        runner: "elf",
    },
    use: [
        // This is the broken use
        {
            protocol: "fuchsia.foo.bar",
            from: "parent",
        },

        // This is a successful use
        {
            protocol: "fuchsia.logger.LogSink",
            from: "parent",
        },
    ],
}
