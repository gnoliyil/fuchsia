import("//build/components.gni")

executable("bin") {
  output_name = "dsputil"
  sources = [ "dsputil.cc" ]
  deps = [
    "//sdk/fidl/fuchsia.hardware.dsp:fuchsia.hardware.dsp_cpp_wire",
    "//sdk/lib/component/incoming/cpp",
    "//sdk/lib/fdio",
    "//zircon/system/ulib/fbl",
    "//zircon/system/ulib/zx",
  ]
}

fuchsia_shell_package("dsputil") {
  deps = [ ":bin" ]
}
