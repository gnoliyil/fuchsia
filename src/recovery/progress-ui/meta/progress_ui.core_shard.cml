{
    children: [
        {
            name: "progress-ui",
            url: "fuchsia-pkg://fuchsia.com/progress-ui#meta/progress_ui.cm",
        },
    ],
    offer: [
        {
            directory: "dev-class",
            from: "parent",
            as: "dev-display-coordinator",
            to: "#progress-ui",
            subdir: "display-coordinator",
        },
        {
            protocol: [
                "fuchsia.logger.LogSink",
                "fuchsia.sysmem.Allocator",
            ],
            from: "parent",
            to: "#progress-ui",
        },
    ],
}
