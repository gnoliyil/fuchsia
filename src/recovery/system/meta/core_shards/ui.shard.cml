{
    offer: [
        {
            protocol: "fuchsia.vulkan.loader.Loader",
            from: "#vulkan_loader",
            to: "#system_recovery",
        },
        {
            directory: "dev-class",
            from: "parent",
            as: "dev-class-display-controller",
            to: "#system_recovery",
            subdir: "display-controller",
        },
        {
            directory: "dev-class",
            from: "parent",
            as: "dev-class-input-report",
            to: "#system_recovery",
            subdir: "input-report",
        },
    ],
}
