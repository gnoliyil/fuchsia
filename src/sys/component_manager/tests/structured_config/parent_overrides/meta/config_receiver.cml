{
    include: [ "syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/config_receiver_bin",
    },
    capabilities: [
        { protocol: "test.config.parentoverrides.Reporter" },
    ],
    expose: [
        {
            protocol: "test.config.parentoverrides.Reporter",
            from: "self",
        },
    ],
    config: {
        parent_provided: {
            type: "string",
            max_size: 1024,
            mutability: [ "parent" ],
        },
    },
}
