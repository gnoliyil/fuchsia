{
    include: [ "syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/nested_reporter_v2",
    },
    children: [
        {
            name: "child_a",
            url: "#meta/stub_component_v2.cm",
            startup: "eager",
        },
        {
            name: "child_b",
            url: "#meta/stub_component_v2.cm",
            startup: "eager",
        },
        {
            name: "child_c",
            url: "#meta/stub_component_v2.cm",
            startup: "eager",
        },
    ],
    use: [
        {
            event_stream: "started",
            from: "parent",
        },
        {
            protocol: [ "fuchsia.sys2.EventSource" ],
        },
    ],
}
