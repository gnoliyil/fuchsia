{
    include: [ "//src/sys/component_manager/tests/services/meta/service-routing-branch.shard.cml" ],
    program: {
        args: [
            "--num-collections",
            "2",
        ],
    },
    collections: [
        {
            name: "account_providers_1",
            durability: "transient",
        },
        {
            name: "account_providers_2",
            durability: "transient",
        },
    ],
    expose: [
        {
            service: "fuchsia.examples.services.BankAccount",
            from: [
                "#account_providers_1",
                "#account_providers_2",
            ],
        },
    ],
}
