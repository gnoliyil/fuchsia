{
    include: [ "syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/process_resolver",
    },
    capabilities: [
        { protocol: "fuchsia.process.Resolver" },
    ],
    use: [
        {
            protocol: [
                "fuchsia.pkg.PackageResolver-base",
                "fuchsia.pkg.PackageResolver-full",
            ],
        },
    ],
    expose: [
        {
            protocol: "fuchsia.process.Resolver",
            from: "self",
        },
    ],
}
