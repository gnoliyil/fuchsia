{
    include: [
        "//sdk/lib/inspect/client.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        runner: "elf",
    },
    capabilities: [
        {
            protocol: [ "fuchsia.fuzzer.TargetAdapter" ],
        },
    ],
    use: [
        { protocol: "fuchsia.fuzzer.CoverageDataCollectorV2" },
    ],
    expose: [
        {
            protocol: [ "fuchsia.fuzzer.TargetAdapter" ],
            from: "self",
        },
    ],
}
