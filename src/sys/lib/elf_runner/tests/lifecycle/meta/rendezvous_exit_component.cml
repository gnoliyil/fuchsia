{
    include: [ "syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/rendezvous_exit",
    },
    capabilities: [
        { protocol: "fidl.test.components.Trigger" },
    ],
    expose: [
        {
            protocol: "fuchsia.component.Binder",
            from: "framework",
        },
        {
            protocol: "fidl.test.components.Trigger",
            from: "self",
        },
    ],
}
