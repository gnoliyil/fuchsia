{
    include: [ "syslog/client.shard.cml" ],
    program: {
        runner: "elf",
        binary: "bin/prints_when_launched_cpp",
        forward_stderr_to: "log",
        forward_stdout_to: "log",
    },
    expose: [
        {
            protocol: "fuchsia.component.Binder",
            from: "framework",
        },
    ],
}
