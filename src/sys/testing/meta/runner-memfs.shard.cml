{
    children: [
        {
            name: "runner_memfs",
            url: "fuchsia-pkg://fuchsia.com/test_manager#meta/memfs.cm",
        },
    ],
    capabilities: [
        {
            storage: "memfs_data",
            from: "#runner_memfs",
            backing_dir: "memfs",
            storage_id: "static_instance_id_or_moniker",
        },
    ],
    offer: [
        {
            protocol: "fuchsia.inspect.InspectSink",
            from: "parent",
            to: [ "#runner_memfs" ],
        },
    ],
}
