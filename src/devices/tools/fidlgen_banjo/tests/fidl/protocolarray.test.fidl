library banjo.examples.protocolarray;

// TODO(surajmalhotra): Async for arrays.
// TODO(surajmalhotra): Reference for arrays.

using zx;

@transport("Banjo")
@banjo_layout("ddk-protocol")
closed protocol Array {
    strict Bool(struct {
        b array<bool, 1>;
    }) -> (struct {
        b array<bool, 1>;
    });
    strict Int8(struct {
        i8 array<int8, 1>;
    }) -> (struct {
        i8 array<int8, 1>;
    });
    strict Int16(struct {
        i16 array<int16, 1>;
    }) -> (struct {
        i16 array<int16, 1>;
    });
    strict Int32(struct {
        i32 array<int32, 1>;
    }) -> (struct {
        i32 array<int32, 1>;
    });
    strict Int64(struct {
        i64 array<int64, 1>;
    }) -> (struct {
        i64 array<int64, 1>;
    });
    strict Uint8(struct {
        u8 array<uint8, 1>;
    }) -> (struct {
        u8 array<uint8, 1>;
    });
    strict Uint16(struct {
        u16 array<uint16, 1>;
    }) -> (struct {
        u16 array<uint16, 1>;
    });
    strict Uint32(struct {
        u32 array<uint32, 1>;
    }) -> (struct {
        u32 array<uint32, 1>;
    });
    strict Uint64(struct {
        u64 array<uint64, 1>;
    }) -> (struct {
        u64 array<uint64, 1>;
    });
    strict Float32(struct {
        f32 array<float32, 1>;
    }) -> (struct {
        f32 array<float32, 1>;
    });
    strict Float64(struct {
        u64 array<float64, 1>;
    }) -> (struct {
        f64 array<float64, 1>;
    });
    strict Handle(resource struct {
        u64 array<zx.handle, 1>;
    }) -> (resource struct {
        f64 array<zx.handle, 1>;
    });
};

const array_size uint32 = 32;

