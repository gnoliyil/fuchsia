{
    children: [
        {
            name: "radar-proxy",
            url: "fuchsia-pkg://fuchsia.com/radar-proxy-without-injector#meta/radar-proxy.cm",
        },
    ],
    use: [
        {
            protocol: "fuchsia.hardware.radar.RadarBurstReaderProvider",
            from: "#radar-proxy",
        },
    ],
    offer: [
        {
            directory: "dev-class",
            from: "parent",
            as: "dev-radar",
            to: "#radar-proxy",
            subdir: "radar",
        },
        {
            protocol: "fuchsia.logger.LogSink",
            from: "parent",
            to: "#radar-proxy",
        },
    ],
}
