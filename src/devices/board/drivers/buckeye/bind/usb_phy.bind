composite usb_phy;

using fuchsia.hardware.registers;
using fuchsia.amlogic.platform;

primary node "register-reset" {
  fuchsia.BIND_FIDL_PROTOCOL == fuchsia.hardware.registers.BIND_FIDL_PROTOCOL.DEVICE;
  fuchsia.BIND_REGISTER_ID == fuchsia.amlogic.platform.BIND_REGISTER_ID.USB_PHY_V2_RESET;
}
