composite ti_ina231_mlb_proto;

using fuchsia.i2c;
using fuchsia.ti.platform;

primary node "i2c" {
  fuchsia.BIND_FIDL_PROTOCOL == fuchsia.i2c.BIND_FIDL_PROTOCOL.DEVICE;
  fuchsia.BIND_I2C_BUS_ID == fuchsia.i2c.BIND_I2C_BUS_ID.I2C_3;
  fuchsia.BIND_I2C_ADDRESS == fuchsia.ti.platform.BIND_I2C_ADDRESS.INA231_MLB_PROTO;
}
