library fuchsia.reloaddriver.test;

using zx;

const MAX_FROM_LENGTH uint64 = 32;

@discoverable
closed protocol Waiter {
    strict Ack(struct {
        from_node string:MAX_FROM_LENGTH;
        from_name string:MAX_FROM_LENGTH;
        status zx.status;
    });
};
