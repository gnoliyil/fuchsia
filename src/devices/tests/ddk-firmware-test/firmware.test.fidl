library fuchsia.device.firmware.test;

using zx;

closed protocol TestDevice {
    /// Call load_firmware and make sure it worked.
    strict LoadFirmware(struct {
        path string;
    }) -> () error zx.status;
    /// Call load_firmware_async and make sure it worked.
    strict LoadFirmwareAsync(struct {
        path string;
    }) -> () error zx.status;
};
