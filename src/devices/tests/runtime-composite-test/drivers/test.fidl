library fuchsia.runtime.composite.test;

using zx;

@discoverable
@transport("Driver")
closed protocol RuntimeCompositeProtocol {
    strict Handshake() -> () error zx.status;
};

service Service {
    runtime_composite_protocol client_end:RuntimeCompositeProtocol;
};
