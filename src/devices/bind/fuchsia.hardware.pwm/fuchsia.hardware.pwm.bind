library fuchsia.hardware.pwm;

extend uint fuchsia.BIND_FIDL_PROTOCOL {
  DEVICE = 24,
};

extend uint fuchsia.BIND_INIT_STEP {
  GPIO = 0x4F495047,
  PWM = 0x004D5750,
};

enum PWM_ID_FUNCTION {
  AMBER_LED,
  GREEN_LED,
  CORE_POWER_BIG_CLUSTER,
  CORE_POWER_LITTLE_CLUSTER,
  LCD_BRIGHTNESS,
};
