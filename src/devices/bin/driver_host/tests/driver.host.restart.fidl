library fuchsia.device.restarttest;

using zx;

/// Interface for communication between tests and the test device.
closed protocol TestDevice {
    /// Get the pid of the driver.
    strict GetPid() -> (struct {
        pid zx.Koid;
    }) error zx.Status;
};
