{
    include: [
        "sys/component/realm_builder.shard.cml",
        "syslog/client.shard.cml",
    ],
    use: [
        {
            event_stream: [
                "started",
                "stopped",
            ],
            from: "parent",
        },
    ],
    offer: [
        {
            storage: "data",
            from: "parent",
            to: [ "#realm_builder" ],
        },
    ],
}
