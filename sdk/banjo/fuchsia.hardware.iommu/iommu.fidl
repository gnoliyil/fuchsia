library fuchsia.hardware.iommu;

using zx;

@transport("Banjo")
@banjo_layout("ddk-protocol")
protocol Iommu {
    GetBti(struct {
        iommu_index uint32;
        bti_id uint32;
    }) -> (resource struct {
        s zx.Status;
        handle zx.Handle:BTI;
    });
};
