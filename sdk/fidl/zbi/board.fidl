library zbi;

const BOARD_NAME_LEN uint64 = 32;

/// ZBI_TYPE_PLATFORM_ID payload.
type PlatformId = struct {
    vid uint32;
    pid uint32;
    board_name string_array<BOARD_NAME_LEN>;
};

/// ZBI_TYPE_DRV_BOARD_INFO payload.
type BoardInfo = struct {
    revision uint32;
};
