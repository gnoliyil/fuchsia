library fuchsia.kernel;

using zx;

/// Protocol for providing an IRQ resource with access to all valid ranges.
@discoverable
closed protocol IrqResource {
    /// Get an IRQ resource handle.
    strict Get() -> (resource struct {
        resource zx.Handle:RESOURCE;
    });
};
