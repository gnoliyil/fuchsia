library fuchsia.kernel;

using zx;

/// Protocol for providing an SMC resource with access to all valid ranges.
@discoverable
protocol SmcResource {
    /// Get an SMC resource handle.
    Get() -> (resource struct {
        resource zx.Handle:RESOURCE;
    });
};
