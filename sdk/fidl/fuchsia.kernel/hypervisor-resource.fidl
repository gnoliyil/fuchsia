library fuchsia.kernel;

using zx;

/// Protocol for providing the hypervisor resource.
@discoverable
protocol HypervisorResource {
    /// Get a hypervisor resource handle.
    Get() -> (resource struct {
        resource zx.Handle:RESOURCE;
    });
};
