library fuchsia.starnix.container;

using zx;

@discoverable
protocol Controller {
    /// Connects `bridge_socket` to a vsocket at `port` in the container.
    VsockConnect(resource struct {
        port uint32;
        bridge_socket zx.handle:SOCKET;
    });
};
