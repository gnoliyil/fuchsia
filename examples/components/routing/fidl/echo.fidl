// [START fidl_echo]
library fidl.examples.routing.echo;

const MAX_STRING_LENGTH uint64 = 64;

@discoverable
protocol Echo {
    /// Returns the input.
    EchoString(struct {
        value string:<MAX_STRING_LENGTH, optional>;
    }) -> (struct {
        response string:<MAX_STRING_LENGTH, optional>;
    });
};
// [END fidl_echo]
