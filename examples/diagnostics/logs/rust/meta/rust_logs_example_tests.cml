{
    include: [
        "//src/sys/test_runners/rust/default.shard.cml",
        "syslog/client.shard.cml",
    ],
    program: {
        binary: "bin/rust_logs_example_tests",
    },
    children: [
        {
            name: "logs_example",
            url: "#meta/rust_logs_example.cm",
        },
    ],
    use: [
        {
            protocol: [
                "fuchsia.diagnostics.ArchiveAccessor",
                "fuchsia.logger.Log",
            ],
        },
        {
            protocol: "fuchsia.component.Binder",
            from: "#logs_example",
        },
    ],
}
