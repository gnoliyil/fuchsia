library fuchsia.examples.inspect;

// [START reverser_fidl]
// Implementation of a string reverser.
@discoverable
protocol Reverser {
    // Returns the input string reversed character-by-character.
    Reverse(struct {
        input string:1024;
    }) -> (struct {
        response string:1024;
    });
};
// [END reverser_fidl]
