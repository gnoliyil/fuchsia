import("//build/components.gni")
import("//build/rust/rustc_test.gni")

rustc_test("stdout_test") {
  name = "stdout_test"
  edition = "2021"
  source_root = "src/stdout_test.rs"

  sources = [ "src/stdout_test.rs" ]
}

fuchsia_unittest_package("stdout-test") {
  deps = [ ":stdout_test" ]
}

group("tests") {
  testonly = true
  deps = [ ":stdout-test" ]
}
