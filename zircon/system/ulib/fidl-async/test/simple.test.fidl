library fidl.test.simple;

closed protocol Simple {
    /// Replies with the value requested.
    strict Echo(struct {
        request int32;
    }) -> (struct {
        reply int32;
    });

    /// Never actually replies.  Just closes instead.
    strict Close() -> (struct {
        v int32;
    });
};
