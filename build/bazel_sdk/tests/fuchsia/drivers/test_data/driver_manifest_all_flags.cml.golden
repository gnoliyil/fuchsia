{
    "program": {
        "binary": "driver/libdriver_no_src.so",
        "bind": "meta/bind/foo.bindbc",
        "colocate": "true",
        "default_dispatcher_opts": [
            "some_opt"
        ],
        "fallback": "true",
        "runner": "driver",
        "default_dispatcher_scheduler_role": "some_role"
    },
    "use": [
        {
            "protocol": "fuchsia.boot.RootResource"
        },
        {
            "protocol": "fuchsia.scheduler.ProfileProvider"
        },
        {
            "protocol": "fuchsia.sysmem.Allocator"
        },
        {
            "protocol": "fuchsia.boot.Arguments"
        }
    ]
}
