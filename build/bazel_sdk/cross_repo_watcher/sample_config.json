{
    "BUILD_WIFI_DRIVER" : {
        "repo": "~/iwlwifi",
        "commands": [
            "tools/bazel build third_party/iwlwifi/platform:iwlwifi_pkg"
        ],
        "triggered_events": [
            "BUILD_CORE",
            "UNDEFINED"
        ]
    },
    "BUILD_CORE" : {
        "repo": "~/fuchsia",
        "commands": [
            "fx clean",
            "fx set core.x64",
            "fx build"
        ],
        "triggered_events": []
    }
}
