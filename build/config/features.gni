# Providing `//build/config/features.gni` to allow ICU to compile on Fuchsia.
# An import was added here:
# https://chromium-review.googlesource.com/c/chromium/deps/icu/+/4215076
declare_args() {
  proprietary_codecs = false
  use_udev = false
  use_dbus = false
  use_gio = false
  use_blink = false
}
